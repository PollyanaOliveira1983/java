package entities;

public class Student {

	public String name;
	public double note1;
	public double note2;
	public double note3;
	
	public double finalNote() {
		return (note1 * 30.0 + note2 * 35.0 + note3 * 35.0) / 100.0;
	}
	
	public double missingNote(double missing) {
		
		if (finalNote() > 60.0 ) {
			System.out.println("Aprovado");
		}
		else {
			missing =  (finalNote() - 60.0); 
		}
	}
	
}
